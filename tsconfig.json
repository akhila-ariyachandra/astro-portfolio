{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/layouts/*": ["src/layouts/*"],
      "@/components/*": ["src/components/*"],
      "@/styles/*": ["src/styles/*"],
      "@/public/*": ["public/*"],
      "@/utils/*": ["src/utils/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}
